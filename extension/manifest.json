{
    "manifest_version": 3,
    "name": "Guardian-X",
    "description": "Guardian-X is a browser extension that helps you to protect your privacy and security while you are browsing the web.",
    "version": "1.0",
    "permissions": [
        "debugger",
        "tabs",
        "downloads",
        "webNavigation"
    ],
    "devtools_page": "devtools.html",
    "host_permissions": [
        "https://www.google.com/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": "guardian.png",
        "default_popup": "guardian.html"
    }
}